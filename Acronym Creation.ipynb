   "source": [
    "user_in = str(input(\"Enter some Input to build Acronym:\"))\n",
    "acronym = user_in.split()\n",
    "if len(acronym)!=1:\n",
    "    count = 0\n",
    "    for i in acronym:\n",
    "        acronym[count] = i[0].upper()\n",
    "        count += 1\n",
    "\n",
    "finals_acr = (\"\").join(acronym)\n",
    "finals_acr"
   ]
